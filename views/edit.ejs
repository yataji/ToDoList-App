<!DOCTYPE html>
<html>
  <head>
    <title>ToDo LIST</title>
  </head>
  <body>
    <form action="/todos/update/<%= data.id %>" method="post">
      <!-- <input type="text" name="id" placeholder="id" /> -->
      <input
        type="text"
        name="title"
        placeholder="title"
        value="<%=data.title%>"
      />
      <input
        type="text"
        name="description"
        placeholder="description"
        value="<%=data.description%>"
      />
      <input type="submit" value="submit" />
    </form>
  </body>
</html>
